{
  "name": "visual-check-testcafe",
  "version": "0.0.0",
  "description": "Testcafe plugin for testing visual regression backed by resemblejs image compare",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/jvdieten/visual-compare-testcafe",
  "author": "Joost van Dieten <joost.van.dieten@testassured.nl> (http://www.testassured.nl)",
  "license": "MIT",
  "keywords": [
    "testcafe",
    "testcafe plugin",
    "visual compare",
    "screenshot compare",
    "testcafe visual",
    "testcafe layout",
    "testcafe image compare",
    "testcafe visual check",
    "visual regression",
    "layout testing",
    "layout regression",
    "visual test",
    "layout test"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.json && cp html-report-template.html build/main/lib",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "watch:build": "tsc -p tsconfig.json -w",
    "version": "standard-version",
    "reset-hard": "git clean -dfx && git reset --hard && npm i"
  },
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "testcafe": "*"
  },
  "dependencies": {
    "canvas": "^2.6.1",
    "handlebars": "^4.7.6",
    "resemblejs": "^3.2.3"
  },
  "devDependencies": {
    "@types/n-readlines": "^1.0.1",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^6.0.1",
    "prettier": "^2.1.1",
    "resemblejs": "^3.2.3",
    "standard-version": "^9.0.0",
    "testcafe": "^1.9.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  }
}
